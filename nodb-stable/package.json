{
  "name": "discord-afk-js",
  "version": "1.5.1",
  "description": "discord-afk-js package is a convenient tool for creating AFK commands without the need for a database",
  "main": "./dist/index.js",
  "types": "./dist/index.ts",
  "type": "commonjs",
  "scripts": {
